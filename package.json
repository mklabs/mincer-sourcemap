{
  "name": "mincer-sourcemap",
  "version": "0.0.0",
  "description": "Takes a mincer compiled script as input stream, outputs the minified bundle with sourcemap",
  "main": "lib/mincer-sourcemap",
  "directories": {
    "bin": "bin"
  },
  "dependencies": {
    "uglify-js2": "~2.1.11",
    "mincer": "~0.4.3",
    "noptify": "latest",
    "debug": "~0.7.0"
  },
  "scripts": {
    "test": "mincer -I ./ examples/todo-backbone/assets/manifest.js | mincer-sourcemap --source-map-prefix examples/todo-backbone:''"
  }
}
